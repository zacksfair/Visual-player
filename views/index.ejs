<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/index.css' />
  </head>
  <body>
	  <header>
	    <h1><%= title %></h1>
	    <P>
	    	Volume<input id="volume" type="range" min="0" max="100 " value="60"></input>
	    </P>
	  </header>
	  <div class="left">
	  	<ul id="list">
	  		<% music.forEach(function(name){ %>
	  			<li title="<%= name %>"><%= name %></li>
	  		<% })%>
	  	</ul>
	  </div>
	  <div class="right"></div>
	  <script type="text/javascript" src="/javascripts/index.js"></script>
  </body>
</html>
